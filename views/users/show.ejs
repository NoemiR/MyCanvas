<body style="background-color: rgb(50, 50, 50);" class="show-page"> 
	 <div class="navbar navbar-default navbar-fixed-top">
		<nav class="navbar" id="index">
			
				<ul class="list-inline-item">
					<li class="list-inline-item">
						<a class="btn btn-primary btn-sm" href="auth/home" role="button">Home</a>
					</li>
					<li class="list-inline-item">
						<a class="btn btn-primary btn-sm" href="/users/index" role="button">Community Canvas</a>
					</li>
					<li class="list-inline-item">
						<a class="btn btn-primary btn-sm" href="/photos/new">Add Photos</a>
					</li>
					<li class="list-inline-item">
						<a class="btn btn-primary btn-sm" href="/photos/edit">Edit Content</a>
					</li>
					<li class="list-inline-item">
						<a class="btn btn-primary btn-sm" href="/users/<%= user.id%>/edit">Edit User Information</a>
